<resources>
    <!-- Profile Page -->
    <!-- Label for wifi only switch -->
    <string name="settings_wifi_top">སྐུད་མེད་དྲ་རྒྱར་མཐུད་དུས་ཕབ་ལེན།</string>
    <!-- Label explaining what wifi only means in detail -->
    <string name="settings_wifi_bottom">སྐུད་མེད་དྲ་རྒྱར་མཐུད་དུས་མ་གཏོགས་ནང་དོན་ཕབ་ལེན་མི་བྱེད།</string>
    <!-- Text to clarify that around what is visible -->
    <string name="profile_visibility_off_message">ཁྱེད་ཀྱི་ངོ་སྤྲོད་སྙིང་བསྡུས་ཀྱི་གནས་ཚུལ་རྣམས་སྒེར་ཁོ་ན་མ་གཏོགས་གཞན་གྱིས་མཐོང་མི་ཐུབ།  གཞན་གྱིས་མཐོང་ཐུབ་པ་ནི་ {platform_name}ཁྱེད་ཀྱི་སྤྱོད་མིང་ཁོ་ན་ཡིན།</string>

    <!--  String for Profile Screen  -->
    <!--  Heading of Wifi Field  -->
    <string name="label_wifi">སྐུད་མེད་དྲ་རྒྱ།</string>
    <!--  Heading of Personal Information  -->
    <string name="label_personal_information">སྒེར་གྱི་གནས་ཚུལ།</string>
    <!--  Heading of Help Field  -->
    <string name="label_help">རམ་འདེགས།</string>
    <!--  Description of Submit feedback Field  -->
    <string name="description_submit_feedback">ཁྱེད་ཀྱིས་ང་ཚོའི་མཉེན་ཆས་ནང་ཁྱད་ཆོས་གསར་སྣོན་ནམ་ཡང་ན་བསམ་འཆར་གཞན་གནང་འདོད་ཡོད་དམ།</string>
    <!--  Label of Submit feedback Button  -->
    <string name="label_submit_feedback_btn">རམ་འདེགས་ཚན་པ་ལ་གློག་འཕྲིན་ཐོངས།</string>
    <!--  Heading of Get Support Field  -->
    <string name="title_get_support">རམ་འདེགས་ཚན་པ།</string>
    <!--  Description of Get Support Field  -->
    <string name="description_get_support">འདི་ནས་{platform_name} ཁ་པར་མཉེན་ཆས་བཀོལ་སྟངས་དང་དེའི་ཐད་ཀྱི་ཤེས་བྱ་ལ་སློབ་སྦྱོང་བྱེད་འདོད་ན། རྒྱུན་ལྡན་གྱི་དྲི་བ་དྲིས་ལན་གྱི་སྡེ་ཚན་ལ་གཟིགས་རོགས།</string>
    <!--  Label of Get Support Button  -->
    <string name="label_view_faq_btn">རྒྱུན་ལྡན་གྱི་དྲི་བ་དྲིས་ལན་གྱི་སྡེ་ཚན་སྟོན།</string>
    <!--  Label of Sign out Button  -->
    <string name="label_sign_out_btn">ཕྱིར་ཐོན།</string>
    <!-- Delete my account screen Title -->
    <string name="title_delete_my_account">ཁྱེད་ཀྱི་ཁ་བྱང་སུབ།</string>
    <!--  Label of Delete Account Button  -->
    <string name="label_delete_account_btn">ཁ་བྱང་སུབ།</string>
    <!--  Description of Delete Account  -->
    <string name="description_delete_account">འཆར་ངོས་རྗེས་མར་གཟིགས་ཏེ་རང་ཉིད་ཀྱི་ཁ་བྱང་དང་དེ་དང་འབྲེལ་བའི་གཞི་གྲངས་ཡོངས་བསུབ་སྟངས་ཀྱི་ལམ་སྟོན་ཁག་ལག་བསྟར་བྱོས།</string>
    <!--  Label of App Version  -->
    <string name="label_app_version">ཐོན་རིམ།</string>
    <!--  Heading of Privacy Fields  -->
    <string name="label_privacy">གསང་རྒྱ།</string>
    <!--  Label of Privacy Policy Button  -->
    <string name="label_privacy_policy">སྒེར་གྱི་གསང་རྒྱ།</string>
    <!--  Label of Cookie Policy Button  -->
    <string name="label_cookie_policy">Cookieཡི་འགྲོ་སྟངས།</string>
    <!--  Label of Data Consent Button  -->
    <string name="label_do_not_sell_my_personal_data">ང་སྒེར་གྱི་གནས་ཚུལ་ཚོང་བསྒྱུར་བྱེད་མི་ཆོག</string>
    <!-- Logout button label -->
    <string name="logout">ཕྱིར་ཐོན།</string>
    <!-- Profile screen version -->
    <string name="label_version">མཉེན་ཆས་ཀྱི་ཐོན་རིམ།</string>

    <!-- User Profiles -->
    <!-- Window title for the user profile screen -->
    <string name="profile_title">ངོ་སྤྲོད་སྙིང་བསྡུས།</string>
    <!-- The accessibility description for the email section -->
    <string name="profile_email_description">གློག་འཕྲིན་ནི་ {email}</string>
    <!-- The accessibility description for the username section -->
    <string name="profile_username_description">སྤྱོད་མིང་ནི་ {username}</string>
    <!-- The accessibility description for the language section -->
    <string name="profile_language_description">སྐད་ཡིག་ནི་ {language}</string>
    <!-- The accessibility description for the location section -->
    <string name="profile_location_description">ས་གནས་ནི་ {location}</string>
    <!-- The accessibility description for the "About Me" section -->
    <string name="profile_about_me_description">ངའི་སྐོར་ནི་ {about_me}</string>
    <!-- Label for toolbar icon that launches the edit profile screen for the current user -->
    <string name="profile_toolbar_edit_button">རྩོམ་སྒྲིག</string>
    <!-- Shown when viewing your own profile and you are not sharing a full profile -->
    <string name="profile_sharing_limited_by_you">ཁྱེད་ཀྱིས་ད་སྔ་རང་ཉིད་ཀྱི་ངོ་སྤྲོད་ཧ་ཅང་བསྡུས་པ་ཞིག་མ་གཏོགས་བཞག་མི་འདུག</string>
    <!-- Shown when viewing another user's profile and that user is not sharing a full profile -->
    <string name="profile_sharing_limited_by_other_user">མི་སྣ་འདིས་རང་ཉིད་ཀྱི་ངོ་སྤྲོད་ཧ་ཅང་བསྡུས་པ་ཞིག་མ་གཏོགས་བཞག་མི་འདུག</string>
    <!-- Shown when viewing your own profile until you have confirmed you are over the age of consent (13 years old) -->
    <string name="profile_consent_needed_explanation">འདིར་རང་ཉིད་ཀྱི་ངོ་སྤྲོད་ཆ་ཚང་བ་འགོད་མཁན་གྱི་ལོ་ཚད་ནི་ལོ་༡༣ ཡན་ལོན་པ་ཞིག་ངེས་པར་དུ་ཡིན་དགོས།</string>
    <!-- Shown when viewing your own incomplete profile -->
    <string name="profile_incomplete_explanation">ཁྱེད་ཀྱིས་ངོ་སྤྲོད་ཚན་པ་དེ་ད་དུང་ཆ་ཚང་ཞིག་འགེང་མེད་པ་འདྲ། </string>
    <!-- Shown as button below the explanation for incomplete profile; it launches the edit profile screen -->
    <string name="profile_incomplete_edit_button">མགོ་རྩོམ།</string>
    <!-- This text is shown in place of the "About Me" text if the user has not filled in their "About Me" -->
    <string name="profile_incomplete_about_me">“ངའི་སྐོར་” གྱི་ཚན་པ་མཁོ་སྤྲོད་བྱས་མི་འདུག</string>
    <!-- Window title for edit profile screen -->
    <string name="edit_user_profile_title">ངོ་སྤྲོད་སྙིང་བསྡུས་རྩོམ་སྒྲིག</string>
    <!-- How most profile fields are formatted when viewing the edit screen -->
    <string name="edit_user_profile_field">{label}: {value}</string>
    <!-- Default placeholder if a profile field is blank -->
    <string name="edit_user_profile_field_placeholder">འདེམ།</string>
    <!-- Button that saves changes to a profile text field -->
    <string name="edit_user_profile_textarea_submit_button">འབུལ་སྤྲོད།</string>
    <!-- An option that will set the user profile's primary language to the language of their device's configured locale -->
    <string name="edit_user_profile_current_language">ད་ལྟའི་སྐད་ཡིག་ནི་ {current_language}</string>
    <!-- An option that will set the user profile's location to the country of their device's configured locale -->
    <string name="edit_user_profile_current_location">ད་ལྟའི་ས་གནས་ནི་ {current_location}</string>
    <!-- Button label that will open gallery or camera for choosing new profile photo -->
    <string name="edit_user_profile_change_photo">བརྗེ་བསྒྱུར།</string>
    <!-- Content Description for change photo button -->
    <string name="edit_user_profile_change_photo_description">ངོ་སྤྲོད་སྙིང་བསྡུས་ཀྱི་འདྲ་པར་བརྗེ་བསྒྱུར།</string>
    <!-- Button that will open a camera for taking a new profile photo -->
    <string name="edit_user_profile_take_photo">འདྲ་པར་རྒྱོབས།</string>
    <!-- Button that will open photo gallery for choosing a new profile photo -->
    <string name="edit_user_profile_choose_photo">འདྲ་པར་ཞིག་འདེམ།</string>
    <!-- Button that will open a camera for taking a new profile photo -->
    <string name="edit_user_profile_remove_photo">འདྲ་པར་སུབ།</string>
    <!-- Title when cropping newly selected profile photo -->
    <string name="edit_user_profile_photo_crop_title">གཏུབ་པ་དང་ཆེ་ཆུང་སྒྲིག</string>
    <!-- Button that will exit the crop tool without saving the image -->
    <string name="edit_user_profile_photo_crop_cancel">ཕྱིར་འཐེན།</string>
    <!-- Button that will confirm the crop region and continue to upload the cropped photo -->
    <string name="edit_user_profile_photo_crop_save">འདེམ།</string>
    <!-- Tab title for section of profile that displays a user's bio -->
    <string name="profile_tab_bio">སྐོར།</string>
    <!-- Tab title for section of profile that lists a user's accomplishments -->
    <string name="profile_tab_accomplishment">ཐུགས་རྗེ་ཆེ།</string>
</resources>
