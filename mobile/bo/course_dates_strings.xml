    <!-- region: Sync to calendar -->
    <!-- Title Sync to calendar  -->
    <string name="sync_to_calendar">ལོ་ཐོ་དང་མཉམ་སྒྲིག་བྱེད།</string>
    <!-- Title Sync to calendar  -->
    <string name="sync_to_calendar_message">སློབ་ཚན་འདིའི་དུས་བཀག་ཡོངས་དང་དུས་བཀག་ཟླ་ཚེས་དང་བཅས་པ་རང་འགུལ་གྱིས་ཁྱེད་ཀྱི་ལོ་ཐོ་དང་སྦྲེལ་འགྲོ་བ་ཡིན།</string>
    <!-- Title to request calendar permission  -->
    <string name="title_request_calendar_permission">\“{platform_name}\” ཁྱེད་ཀྱི་ལོ་ཐོ་སྔོགས་འདོད་འདུག་གམ།</string>
    <!-- Message to request calendar permission  -->
    <string name="message_request_calendar_permission">དྲ་ཚིགས་{platform_name} འདིས་ཁྱེད་ཀྱི་ལོ་ཐོའི་ཐོ་གཞུང་བཀོལ་སྤྱོད་བྱས་ཏེ་སློབ་ཚན་འདིའི་ནང་ཁྱེད་རང་སྒེར་གྱིས་བཟོས་པའི་{platform_name} དུས་ཚོད་རེའུ་མིག་གི་ལོ་ཐོ་དེ་སྔོན་མངགས་བྱེད་འདོད་འདུག</string>
    <!-- Title to add calendar for course  -->
    <string name="title_add_course_calendar">ལོ་ཐོ་སྣོན། \“{course_name}\”</string>
    <!-- Message to add calendar for course  -->
    <string name="message_add_course_calendar">ཁྱེད་རང་ {platform_name} འདི་ལོ་ཐོ་\“{course_name}\” ལ་སྣོན་འདོད་འདུག་གམ། \n\nཁྱེད་ཀྱིས་ནམ་ཡིན་རུང་རང་གི་སློབ་ཚན་ལོ་ཐོ་དེ་ལོ་ཐོ་དང་སྒྲིག་འགོད་གཉིས་ཀྱི་ནང་ནས་བསྐྱར་སྒྲིག་དང་ཡང་ན་བསུབ་པ་གཉིས་ཆ་བྱེད་ཐུབ།</string>
    <!-- Success Message after calendar for course added first time -->
    <string name="message_for_alert_after_course_calendar_added">\“{course_name}\” འདི་ཁྱེད་ཀྱི་ཁ་པར་གྱི་ལོ་ཐོའི་ནང་བསྣན་ཟིན་འདུག</string>
    <!-- Success Message after calendar for course added -->
    <string name="message_after_course_calendar_added">ཁྱེད་ཀྱི་སློབ་ཚན་གྱི་ལོ་ཐོ་བསྣན་ཟིན།</string>
    <!-- Title to remove calendar for course  -->
    <string name="title_remove_course_calendar">ལོ་ཐོ་སུབ། \“{course_name}\”</string>
    <!-- Message to remove calendar for course  -->
    <string name="message_remove_course_calendar">ཁྱེད་ཀྱིས་ {platform_name} ལོ་ཐོ་ \“{course_name}\” བསུབ་འདོད་འདུག་གམ།</string>
    <!-- Success Message after calendar for course removed -->
    <string name="message_after_course_calendar_removed">ཁྱེད་ཀྱི་སློབ་ཚན་གྱི་ལོ་ཐོ་བསུབ་ཟིན།</string>
    <!-- Success Message after calendar for course update -->
    <string name="message_after_course_calendar_updated">ཁྱེད་ཀྱི་སློབ་ཚན་གྱི་ལོ་ཐོ་བསྐྱར་གསོ་བྱས་ཟིན།</string>
    <!-- Title course calendar is out-of-date  -->
    <string name="title_calendar_out_of_date">ཁྱེད་ཀྱི་སློབ་ཚན་གྱི་ལོ་ཐོའི་དུས་ཡུན་རྫོགས་འདུག</string>
    <!-- Message course calendar is out-of-date  -->
    <string name="message_calendar_out_of_date">ཁྱེད་ཀྱི་སློབ་ཚན་གྱི་དུས་ཚོད་འགྱུར་བ་གཏང་འདུག་མོད། ཁྱོད་ཀྱི་སློབ་ཚན་གྱི་ལོ་ཐོ་དེ་དུས་ཚོད་གསར་པའི་རེའུ་མིག་བཞིན་བསྐྱར་སྒྲིག་བྱས་མི་འདུག</string>
    <!-- Error message while creating calendar for the course events -->
    <string name="adding_calendar_error_message">ལོ་ཐོ་གསར་སྣོན་ལ་གནད་དོན་ཞིག་བྱུང་སོང་། རྗེས་ནས་བསྐྱར་དུ་ཚོད་ལྟ་བྱ་རོགས།</string>
    <!-- Alert dialog remove course calendar button -->
    <string name="label_remove_course_calendar">སློབ་ཚན་གྱི་ལོ་ཐོ་བསུབ།</string>
    <!-- Title syncing calendar dialog  -->
    <string name="title_syncing_calendar">ལོ་ཐོ་དང་མཉམ་སྒྲིག་བྱེད་བཞིན་ཡོད་་་</string>
    <!-- endregion -->

    <!-- Message on shift course dates successfully on Assessment -->
    <string name="assessment_shift_dates_success_msg">ཁྱེད་རང་དུས་ལྟར་གནས་ཐུབ་པ་ལ་ཕན་ཐོགས་ཡོང་ཕྱིར་ཁྱེད་ཀྱི་དུས་བཀག་རྣམས་བདེ་ལེགས་ངང་འགྱུར་བ་བཏང་ཟིན།</string>
    <!-- Label to view shifted dates -->
    <string name="assessment_view_all_dates"><u>ཟླ་ཚེས་ཆ་ཚང་སྟོན།</u></string>

    <!-- region: Course dates error -->
    <!-- Error Message when course dates are not available -->
    <string name="course_dates_unavailable_message">ད་སྔ་སློབ་ཚན་གྱི་ཟླ་ཚེས་གང་ཡང་མི་འདུག</string>
    <!-- Message Course Dates Reset Successful -->
    <string name="course_dates_reset_successful">ཁྱེད་ཀྱི་ཟླ་ཚེས་བདེ་ལེགས་ངང་འགྱུར་བ་བཏང་ཟིན།</string>
    <!-- Message Course Dates Reset Unsuccessful -->
    <string name="course_dates_reset_unsuccessful">ཁྱེད་ཀྱི་ཟླ་ཚེས་རྣམས་འགྱུར་བ་གཏོང་ཉན་ས་མ་རེད། བསྐྱར་དུ་ཚོད་ལྟ་བྱེད་རོགས།</string>
    <!-- endregion -->
</resources>
