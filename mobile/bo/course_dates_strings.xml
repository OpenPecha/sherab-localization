    <string name="date_type_today">དེ་རིང་།</string>
    <!-- Label used when an assignment is for verified user only -->
    <string name="date_type_verified_only">ཁུངས་སྐྱེལ་བྱས་པ་ཁོ་ན།</string>
    <!-- Label used when an assignment is completed -->
    <string name="date_type_completed">ཆ་ཚང་གྲུབ་ཟིན་པ།Completed</string>
    <!-- Label used when an assignment is past due -->
    <string name="date_type_past_due">དུས་བཀག་ཡོལ་ཟིན་པ།</string>
    <!-- Label used when an assignment is due next -->
    <string name="date_type_due_next">དུས་བཀག་རྗེས་མ།</string>
    <!-- Label used when an assignment is not yet released -->
    <string name="date_type_not_yet_released">ད་དུང་སྤེལ་མི་འདུག</string>
